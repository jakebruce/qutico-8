function _init()
	player=make_player(63,63)
	cam_xy={player.x,player.y}
	cam_tau=0.1
	entities={player}
	music(0)
end
-->8
--updates

function _update60()
	update_camera()
	for e in all(entities) do
		e:update()
	end
end

function update_camera()
	local x,y=player.x,player.y
	cam_xy={
		x*cam_tau+cam_xy[1]*(1-cam_tau),
		y*cam_tau+cam_xy[2]*(1-cam_tau),
	}
end

function update_player(s)
	if     btn(⬅️) then
		s.x-=s.speed
		s.fx=true
	end if btn(➡️) then
	 s.x+=s.speed
	 s.fx=false
	end if btn(⬆️) then
	 s.y-=s.speed
	 s.fy=false
	end if btn(⬇️) then
	 s.y+=s.speed
	 s.fy=true
	end
	
	if (btnp(🅾️)) sfx(0)
	if (btnp(❎)) sfx(1)
end
-->8
--draws

function _draw()
	cls()
	camera(cam_xy[1]-63,cam_xy[2]-63)
	map()
	for e in all(entities) do
		e:draw()
	end
end

function draw_player(s)
	spr(s.sprite,s.x,s.y,s.w,s.h,s.fx,s.fy)
end
-->8
--entities

function make_player(x,y)
	return {
		x=x,
		y=y,
		speed=2,
		sprite=1,
		w=1,
		h=1,
		fx=false,
		fy=false,
		update=update_player,
		draw=draw_player,
	}
end
