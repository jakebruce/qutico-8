pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init()
	_pal={0,1,131,3,4,5,6,134,8,9,133,11,12,139,14,15}
	for i,c in pairs(_pal) do
		pal(i-1,c,1)
	end
	poke(0x5f2e,1)
	
	x=22
	y=80
	t=1
	f=⬇️
end

function no_buttons()
	return not (btn(⬅️) or btn(➡️) or btn(⬆️) or btn(⬇️))
end

function _update60()
	for d in all({⬅️,➡️,⬆️,⬇️}) do
		if (btn(d)) f=d
	end
	if (btn(⬅️)) x-=1
	if (btn(➡️)) x+=1
	if (btn(⬆️)) y-=1
	if (btn(⬇️)) y+=1
	if (btnp(❎)) interact()
	if no_buttons() then
		t=1
	else
		t+=1
	end
end

function interact()
	
end

function _draw()
	cls()
	map()
	camera(x-63,y-63)
	n=6+(flr(t/8)%4)*2
	spr(n,x,y,2,2)
	if (f==⬅️) dx,dy=-1,0
	if (f==➡️) dx,dy=1,0
	if (f==⬆️) dx,dy=0,-1
	if (f==⬇️) dx,dy=0,1
	spr(16,
		flr((x+16*(dx+1))/16)*16,
		flr((y+16*(dy))/16)*16,
		1,1,false,false)
end
