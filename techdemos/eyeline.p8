--updates

function _update60()
	for e in all(entities) do
		e:update()
	end
end

function update_player(s)
	if (btn(⬅️)) s.x-=s.v
	if (btn(➡️)) s.x+=s.v
	if (btn(⬆️)) s.y-=s.v
	if (btn(⬇️)) s.y+=s.v
	collide_with_map(s)
end
