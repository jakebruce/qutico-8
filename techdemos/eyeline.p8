--collisions

function occupied(x,y,pt)
	x=x+pt[1]
	y=y+pt[2]
	mx=x/8
	my=y/8
	return fget(mget(mx,my),1)	
end

function collide_with_map(s)
	pts={
		{0,4,-1,0},
		{4,0,0,-1},
		{7,4,1,0},
		{4,7,0,1},
	}
	for pt in all(pts) do
		while occupied(s.x,s.y,pt) do	
			s.x-=pt[3]
			s.y-=pt[4]
		end
	end
end
