--debug utils


function draw_spiral()
	rect(0,0,127,127,8)
	for i=1,8 do
		circ(64,64,i*8,8)
	end
	px=64
	py=64
	for n=1,17*17 do
		x=spiral_coords_x[n]*8+64
		y=spiral_coords_y[n]*8+64
		line(px,py,x,y,(-n+flr(time()*10))%15+1)
		px=x
		py=y
	end
end

-->8
--line of sight

function get_visible()
	p=player
	px=p.x+4
	py=p.y+4
	pmx=flr(px/8)
	pmy=flr(py/8)
	
	--visible
	v={}
	
	--player might be on obscured
	--tile
	v[8*__ss+8]=not fget(mget(pmx,pmy),0)
	
	--transparent
	t={}
	t[8*__ss+8]=not fget(mget(pmx,pmy),0)
	
	rt2=sqrt(2.0)
	for n=1,__ss*__ss do
		--map delta from 0,0
		dmx=spiral_coords_x[n]
		dmy=spiral_coords_y[n]
		
		--pixel delta to center
		cmx=dmx*8+4
		cmy=dmy*8+4
		
		--pixel delta to player cntr
		dx=4-cmx
		dy=4-cmy
		
		--pixel distance to player
		d=sqrt(dx*dx+dy*dy)
		
		--unit vector to player
		ux=dx/(d+0.01)
		uy=dy/(d+0.01)
		
		--one step of length sqrt(2)*8
		--toward player
		prex=cmx+ux*rt2*8
		prey=cmy+uy*rt2*8
		
		--map coord of tile we landed on
		premx=flr(prex/8)
		premy=flr(prey/8)
		
		--flat idx of map tile	relative to player tile
		mi=flr(cmy/8+8)*__ss+flr(cmx/8+8)
		
		--flat idx of landed tile relative to player
		pi=(premy+8)*__ss+premx+8
		
		--transparency of current tile
		t[mi]=not fget(mget(pmx+dmx,pmy+dmy),0)
		
		--visibility of current tile
		--is pre-tile transparent and
		--pre-tile visible
		v[mi]=t[pi] and v[pi]
		
		--did we fuck up?
		if (t[pi]==nil or v[pi]==nil) then
			print("idx "..mi.." "..pi)
			print("cm "..cmx.." "..cmy)
			print("prem "..premx.." "..premy)
	 	print("d "..dx.." "..dy.." "..d)
	 	print("p "..px.." "..py)
	 	print("u "..ux.." "..uy)
	 	print("urt2 "..(ux*rt2).." "..(uy*rt2))
	 	print("dm "..dmx.." "..dmy)
	 	print("pre "..prex.." "..prey)
	 	stop()
		end
	end
	return v
end

