--eyeline
--by jake

function _init()
	player=make_player(63,63)
	entities={player}
	
	spiral_coords_x={}
	spiral_coords_y={}
	__ss=17
	for n=1,__ss*__ss do
	 c=_spiral_coord(n)
	 spiral_coords_x[n]=c[1]
	 spiral_coords_y[n]=c[2]
	end
end

--returns the spiral coordinate
--at index n; n=0 is (0,0)
function _spiral_coord(n)
 k=ceil((sqrt(n)-1)/2)
 t=2*k+1
 m=t^2 
 t=t-1
 if n>=m-t then
 	return {k-(m-n),-k}
 else
 	m=m-t end
 if n>=m-t then
 	return {-k,-k+(m-n)}
 else m=m-t end
 if n>=m-t then
 	return {-k+(m-n),k}
 else
		return {k,k-(m-n-t)} end
end
