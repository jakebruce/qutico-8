--draws

function _draw()
	cls()
	camera(player.x-63,player.y-63)
 draw_map()
	for e in all(entities) do
		e:draw()
	end
end

function draw_map()
	p=player
	px=p.x+4
	py=p.y+4
	pmx=flr(px/8)
	pmy=flr(py/8)
	
	visible_map=get_visible()
	for i,v in pairs(visible_map) do
		if v then
			mx=i%__ss
			my=i/__ss
			_off=0
			if (mx<=0) _off=-1 --hack
			t=mget(pmx+mx-8,pmy+my-8)
			if t>0 then
				spr(t,(pmx+mx-8)*8,(pmy+my-8-mx/16)*8+1+_off)
			end
		end
	end
end

function draw_player(s)
	float=sin(time()/1.5)
	spr(s.n,s.x,flr(s.y)+0.5+float)
end
